{
  "name": "react-configured",
  "version": "0.0.1-alpha",
  "description": "Configure react components with default props",
  "repository": "github.com:Appello-Pty-Ltd/react-configured.git",
  "homepage": "https://github.com/Appello-Pty-Ltd/react-configured",
  "keywords": [
    "react",
    "config",
    "props",
    "styled"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ],
  "peerDependencies": {
    "react": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/react": "^18.0.28",
    "babel-preset-env": "^1.7.0",
    "prettier": "^2.8.3",
    "react": "18.2.0",
    "rollup": "^3.12.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-ts": "^3.2.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "build:watch": "rollup -c -w",
    "prettier": "prettier --write src/**/*",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "@appello/common": "^1.0.22"
